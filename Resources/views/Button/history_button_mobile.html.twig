{% if admin.hasroute('history') and admin.id(object) and admin.isGranted('EDIT', object) %}
    <li>
        <a class="sonata-action-element" href="{{ admin.generateObjectUrl('history', object) }}">
            <i class="icon-book"></i>
            {{ 'link_action_history'|trans({}, 'SonataAdminBundle') }}</a>
    </li>
{% endif %}
